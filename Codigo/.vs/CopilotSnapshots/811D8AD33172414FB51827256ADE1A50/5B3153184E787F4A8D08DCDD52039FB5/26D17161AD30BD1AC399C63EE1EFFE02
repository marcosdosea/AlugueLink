namespace AlugueLinkWEB;

public partial class Contrato
{
    public int Id { get; set; }

    public DateOnly? DataAssinatura { get; set; }

    public string? Status { get; set; }

    public DateOnly? DataInicio { get; set; }

    public DateOnly? DataTermino { get; set; }

    public int LocatárioId { get; set; }

    public int AluguelId { get; set; }

    public virtual Aluguel Aluguel { get; set; } = null!;

    public virtual Locatário Locatário { get; set; } = null!;
}
